<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="peanutencryption.peanutencryption"
    android:versionCode="1"
    android:versionName="1.0">

    <uses-sdk android:minSdkVersion="10"
        android:targetSdkVersion="22"
        android:maxSdkVersion="23" />
    <application
        android:allowBackup="false"
        android:icon="@mipmap/ic_launcher_circle_big"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Initialization"
            android:label="@string/title_activity_initialization"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".Logg_In"
            android:label="@string/title_activity_start"
            android:windowSoftInputMode="stateVisible"
            android:screenOrientation="portrait"/>
        <activity android:name=".LicenseActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".NewCodeActivity"
            android:screenOrientation="portrait"></activity>
    </application>

</manifest>
